{
  "extends": "airbnb",

  "globals": {
    "React": true,
  },

  "rules": {
    "key-spacing": [2, {
      "singleLine": {
        "beforeColon": false,
        "afterColon": true,
      },
      "multiLine": {
        "beforeColon": false,
        "afterColon": true,
        "align": "value",
      }
    }],
  }
}
